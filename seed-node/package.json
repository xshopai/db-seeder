{
  "name": "xshopai-unified-seeder",
  "version": "1.0.0",
  "description": "Unified data seeding system for xshopai microservices",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "tsx src/index.ts",
    "seed": "tsx src/index.ts seed",
    "seed:users": "tsx src/index.ts seed --service=user-service",
    "seed:products": "tsx src/index.ts seed --service=product-service",
    "seed:inventory": "tsx src/index.ts seed --service=inventory-service",
    "seed:orders": "tsx src/index.ts seed --service=order-service",
    "seed:reviews": "tsx src/index.ts seed --service=review-service",
    "validate:reviews": "tsx src/validate-reviews.ts",
    "clear": "tsx src/index.ts clear",
    "clear:all": "tsx src/index.ts clear",
    "validate": "tsx src/index.ts seed --validate --dry-run",
    "status": "tsx src/index.ts status",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "seeding",
    "database",
    "microservices",
    "mongodb",
    "postgresql",
    "data-initialization"
  ],
  "author": "xshopai Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "dotenv": "^16.3.0",
    "mongodb": "^6.0.0",
    "mssql": "^12.0.0",
    "mysql2": "^3.15.3",
    "pg": "^8.16.3",
    "redis": "^4.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
